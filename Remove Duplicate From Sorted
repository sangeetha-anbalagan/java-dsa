class Solution {
    public int removeDuplicates(int[] nums) {
        // if(nums.length==0)return 0;
        int left=0;
        for(int j=1;j<nums.length;j++){
            if(nums[left]!=nums[j]){
                left++;
                nums[left]=nums[j];
                    
                    //System.out.print(nums[j]);
                }
                
            }
            left++;
            return left;
        
    }
}
